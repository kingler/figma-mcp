# Figma API access token (required)
FIGMA_ACCESS_TOKEN=your_figma_token_here

# Server configuration
PORT=3000
NODE_ENV=development

# Cache settings
CACHE_TTL=300000
CACHE_MAX_ITEMS=500

# Logging
LOG_LEVEL=info

# Rate limiting
RATE_LIMIT_MAX=60
RATE_LIMIT_WINDOW_MS=60000

# Optional: Neo4j connection (for advanced graph-based features)
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=password

# NeDB configuration (for lightweight database storage)
NEDB_DATA_DIR=./data
NEDB_IN_MEMORY=false 